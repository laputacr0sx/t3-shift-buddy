generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shifts {
  id         String @id @default(uuid())
  dutyNumber String
  bNL        String
  bNT        String
  bFT        String
  bFL        String
  duration   String
  remarks    String
}

model WeekPrefix {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  weekNumber Int
  prefixes   String[]
}

enum Location {
  HUH
  SHS
  LOW
  HTD
  TWD
}
